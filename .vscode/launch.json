{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/apps/vscode"
			],
			"outFiles": [
				"${workspaceFolder}/apps/vscode/out/**/*.js"
			],
			"preLaunchTask": "npm: extension:build"
		}
	]
}